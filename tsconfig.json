{
  "compilerOptions": {
    "target": "ES2017",                                
    "module": "commonjs",                              
    "outDir": "./dist",                                  
    "removeComments": true,                         
    "esModuleInterop": true,                                                   
    "forceConsistentCasingInFileNames": true,           
    "strict": true,                                      
    "noUnusedLocals": true,                           
    "noUnusedParameters": false,                       
    "skipLibCheck": true                                
  }
}
